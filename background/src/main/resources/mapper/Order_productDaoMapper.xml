<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ka.store.pojo.order.Order_product">

    <insert id="insertOrderProduct" parameterType="com.ka.store.pojo.order.Order_product">
        insert into order_product(order_id,commodity_id,merchant_id,num,order_price)
            values(#{order_id},#{commodity_id},#{merchant_id},#{num},#{order_price})
    </insert>

</mapper>